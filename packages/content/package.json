{
  "name": "@howtographql/content",
  "version": "0.0.1",
  "description": "The content/gatsby theme for htg2",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.12",
    "gatsby": "^2.3.24",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "loader-utils": "^1.2.3",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "gatsby": "^2.3.24",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@mdx-js/loader": "^0.20.3",
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "babel-plugin-styled-components": "^1.10.0",
    "gatsby-image": "^2.0.33",
    "gatsby-mdx": "^0.4.3",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-page-creator": "^2.0.12",
    "gatsby-plugin-react-helmet": "^3.0.9",
    "gatsby-plugin-sharp": "^2.0.28",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-remark-prismjs": "^3.2.5",
    "gatsby-source-filesystem": "^2.0.24",
    "gatsby-transformer-remark": "^2.3.2",
    "gatsby-transformer-sharp": "^2.1.17",
    "prismjs": "^1.15.0",
    "prop-types": "^15.7.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
